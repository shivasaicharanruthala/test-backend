{
  "source": {
    "include": ["./controllers/", "./database/", "./models", "./services/", "./utils/"],
    "includePattern": ".js$",
    "excludePattern": ".*test.js$"
  },
  "plugins": [
    "plugins/markdown",
    "jsdoc-route-plugin"
  ],
  "opts" : {
    "encoding": "utf8",
    "readme": "./README.md",
    "destination": "docs/server/",
    "recurse": true
  }
}